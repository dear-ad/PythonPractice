import pywhatkit

lists = [
    {'phone':'+2348172573305', 
    'msg':'Bot!', 'hour': 19, 'min': 58
    }, 

    {'phone':'+2348169689582', 
    'msg':'Bot!', 'hour': 19, 'min': 59
    },

    {'phone':'+2348032679648', 
    'msg':'Bot!', 'hour': 20, 'min': 00
    },

    {'phone':'+2348101353593', 
    'msg':'Bot!', 'hour': 20, 'min': 1

    }

]
for item in lists:
    pywhatkit.sendwhatmsg(item['phone'], item['msg'], item['hour'], item['min'])